<?xml version="1.0" encoding="utf-8"?>
<!--
    Added System Default Fonts Family.
-->
<!-- Android Q familyset version update -->
<fonts-modification version='1'>
    <family customizationType='new-named-family' name="default_lgsmartui">
        <font weight="100" style="normal">Roboto-Thin.ttf</font>
        <font weight="100" style="italic">Roboto-ThinItalic.ttf</font>
        <font weight="300" style="normal">Roboto-Light.ttf</font>
        <font weight="300" style="italic">Roboto-LightItalic.ttf</font>
        <font weight="400" style="normal">Roboto-Regular.ttf</font>
        <font weight="400" style="italic">Roboto-Italic.ttf</font>
        <font weight="500" style="normal">Roboto-Medium.ttf</font>
        <font weight="500" style="italic">Roboto-MediumItalic.ttf</font>
        <font weight="900" style="normal">Roboto-Black.ttf</font>
        <font weight="900" style="italic">Roboto-BlackItalic.ttf</font>
        <font weight="700" style="normal">Roboto-Bold.ttf</font>
        <font weight="700" style="italic">Roboto-BoldItalic.ttf</font>
    </family>
    <family customizationType='new-named-family' name="lgsmartui">
        <font weight="300" style="normal">LG_Smart_UI_Mobile-Light.ttf</font>
        <font weight="400" style="normal">LG_Smart_UI_Mobile-Regular.ttf</font>
        <font weight="700" style="normal">LG_Smart_UI_Mobile-Bold.ttf</font>
    </family>

    <alias name="default_lgsmartui-thin" to="default_lgsmartui" weight="100" />
    <alias name="default_lgsmartui-light" to="default_lgsmartui" weight="300" />
    <alias name="default_lgsmartui-medium" to="default_lgsmartui" weight="500" />
    <alias name="default_lgsmartui-black" to="default_lgsmartui" weight="900" />

    <alias name="lgsmartui-light" to="lgsmartui" weight="300" />
</fonts-modification>
